# Redirect /ai-serviceagent to /aisa.html
RewriteEngine On
RewriteRule ^ai-serviceagent/?$ aisa.html [L]

# Alternative: Direct redirect (301 permanent redirect)
# Redirect 301 /ai-serviceagent https://nasadef.com.my/aisa.html

# For clean URLs without .html extension
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.html [NC,L]

# Remove .html from URLs
RewriteCond %{THE_REQUEST} /([^.]+)\.html [NC]
RewriteRule ^ /%1 [NC,L,R]
